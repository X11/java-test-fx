<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tkeur.OverviewController">
   <content>
      <TableView fx:id="overviewTableView">
        <columns>
          <TableColumn editable="false" minWidth="-1.0" prefWidth="-1.0" text="Name" fx:id="overviewName" />
          <TableColumn editable="false" minWidth="-1.0" prefWidth="-1.0" text="Description" fx:id="overviewDescription" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </content>
</ScrollPane>
